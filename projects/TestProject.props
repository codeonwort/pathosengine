    <LocalDebuggerEnvironment>PATH=$(SolutionDir)thirdparty\binaries;%PATH%$(LocalDebuggerEnvironment)</LocalDebuggerEnvironment>
    <OutDir>$(SolutionDir)bin\$(Configuration)\</OutDir>
    <IntDir>$(SolutionDir)intermediate\$(ProjectName)\$(Configuration)\</IntDir>
