      <TreatWarningAsError>true</TreatWarningAsError>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>$(SolutionDir)\thirdparty\assimp\binary;$(SolutionDir)\thirdparty\freeimage\binary;$(SolutionDir)\thirdparty\freetype-2.9.1\binary;$(SolutionDir)\thirdparty\freeglut\binary</AdditionalLibraryDirectories>
      <IgnoreAllDefaultLibraries>
      </IgnoreAllDefaultLibraries>
    </Link>
    <PostBuildEvent>
      <Command>
      </Command>
    </PostBuildEvent>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>